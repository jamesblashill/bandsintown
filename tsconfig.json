{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "jsx": "react",
    "declaration": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": ["src"]
}
